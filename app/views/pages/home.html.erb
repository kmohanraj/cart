
<h1>Login</h1>

<% form_for :member do |f| %>

 

 
	<p>
		<%= f.label :email %><br>
		<%= f.text_field :email, :size =>20 %><br>
	</p>
	<p>
		<%= f.label :password %><br>
		<%= f.password_field :password, :size => 20 %><br>
	</p>
	<%= f.submit "Login", :controller=> "pages",:action=>"login"%><br />



	<label for="Signup Page!" id="New user!"class="New_user_link">
    	<%= link_to ('Signup Page'),:controller => "members",:action => "signup" %>
    </label>
<% end %>





